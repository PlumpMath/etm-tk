# 1: logging.DEBUG,
# 2: logging.INFO,
# 3: logging.WARN,
# 4: logging.ERROR,
# 5: logging.CRITICAL

# default = 3
    if level in log_levels:
        loglevel = log_levels[level]
    else:
        loglevel = log_levels[3]

    setup_logging(default_level=loglevel)

view.main()
